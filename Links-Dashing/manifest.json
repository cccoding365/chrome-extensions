{
    "manifest_version": 3,
    "name": "Links Dashing",
    "version": "1.0",
    "description": "A simple extension to display user bookmarks.",
    "permissions": [
        "activeTab",
        "scripting",
        "bookmarks",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "64": "images/icon64.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
    }
}